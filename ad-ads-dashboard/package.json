{
  "name": "ad-ads-dashboard",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate --schema=../prisma/schema.prisma && next build",
    "start": "next start -p 3000",
    "start:render": "node -e \"require('child_process').execSync(`next start -p ${process.env.PORT || 3000}`, { stdio: 'inherit' })\"",
    "prisma:generate": "prisma generate --schema=../prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=../prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "start:migrated": "npm run prisma:migrate:deploy && npm run start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "6.18.0",
    "axios": "1.13.1",
    "bcryptjs": "3.0.3",
    "date-fns": "4.1.0",
    "lucide-react": "0.552.0",
    "next": "14.2.12",
    "next-auth": "4.24.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "3.3.0",
    "stripe": "19.2.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.6",
    "prisma": "6.18.0",
    "tailwindcss": "3.4.18",
    "typescript": "^5.9.3"
  }
}
